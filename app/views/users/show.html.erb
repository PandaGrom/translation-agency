<h1 class="mb-4"><%= @user.name %> </h1>
<time datetime="<%= @user.created_at %>">
  <%= @user.created_at %>
</time>
<p class="lead my-3">
  <span>Name: </span>
  <%= @user.name%><br>
  <span>Contacts: </span>
  <%= @user.contacts%><br>
  <span>Email: </span>
  <%= @user.email%><br>
  <span>Role: </span>
  <%= @user.role%><br>
  <span>Blocked: </span>
  <%= @user.blocked%>
</p>
